        cr.line_to(435.76, 137.73)
        cr.stroke()
        cr.move_to(478.07, 96.84)
        cr.line_to(511.44, 94.96)
        cr.stroke()
        cr.move_to(498.98, 0)
        cr.line_to(541.53, 126.92)
        cr.stroke()
        cr.move_to(562.21, 41.84)
        cr.line_to(575.37, 129.27)   
        cr.stroke()
        cr.move_to(661.87, 160.30)
        cr.line_to(620.927, 83.20)   
        cr.stroke()  
