        cr.move_to(92.13, 179.57)
        cr.line_to(98.77, 160.74)
        cr.line_to(20.68, 160.77)
        cr.move_to(394.06, 453.71)
        cr.line_to(390.02, 450.71)
        cr.line_to(396.08, 445.20)
        cr.move_to(90.13, 178.90)
        cr.line_to(96.44, 162.84)
        cr.line_to(94.20, 160.75)
        cr.set_source_rgb(0.5, 1, 105/255)    
        cr.fill()
       
def main():
    app = Window()
    Gtk.main()
if __name__ == "__main__":    
    main()
